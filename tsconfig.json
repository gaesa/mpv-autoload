{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es2015.core", "es2015.collection", "es2019.string"],
    "module": "es6",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noEmitOnError": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "vendor", "dist", "**/*.test.ts"]
}
